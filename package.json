{
  "name": "bireader",
  "version": "1.0.10",
  "description": "Read and write data in binary",
  "types": "./lib/cjs/types/index.d.ts",
  "module": "./lib/esm/index.mjs",
  "main": "./lib/cjs/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hearhellacopters/bireader.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rmdir /s lib",
    "build": "npm run clean && npm run build:esm && npm run move && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "move": "cd ./lib/esm/ && ren index.js index.mjs && cd ../../",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": ""
  },
  "keywords": [
    "buffer",
    "parser",
    "reader",
    "binary",
    "decode",
    "unpack",
    "struct",
    "bit",
    "hex"
  ],
  "author": "HearHellacopters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hearhellacopters/bireader/issues"
  },
  "homepage": "https://github.com/hearhellacopters/bireader#readme",
  "devDependencies": {
    "@types/node": "^20.7.0",
    "typescript": "^5.2.2"
  }
}
