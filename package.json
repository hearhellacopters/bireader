{
  "name": "bireader",
  "version": "1.0.29",
  "description": "Read and write data in binary",
  "module": "lib/esm/index.mjs",
  "main": "lib/cjs/index.js",
  "directories": {
    "lib": "src"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hearhellacopters/bireader.git"
  },
  "exports": {
    ".": {
      "node": "./lib/esm/index.mjs",
      "import": "./lib/esm/index.mjs",
      "require": "./lib/cjs/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "clean": "rmdir /s lib",
    "build": "npm run clean && npm run build:esm && npm run move && npm run build:cjs",
    "build:esm": "tsc --module esnext --outDir lib/esm",
    "move": "cd ./lib/esm/ && ren index.js index.mjs && cd ../../",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "prepack": ""
  },
  "keywords": [
    "buffer",
    "parser",
    "reader",
    "binary",
    "decode",
    "unpack",
    "struct",
    "bit",
    "hex"
  ],
  "author": "HearHellacopters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hearhellacopters/bireader/issues"
  },
  "homepage": "https://github.com/hearhellacopters/bireader#readme",
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.7.0",
    "@typescript/lib-dom": "npm:@types/web@^0.0.115",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=12"
  }
}
