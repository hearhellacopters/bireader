{
  "name": "bireader",
  "version": "1.0.4",
  "description": "Read and write data in binary",
  "main": "index.js",
  "directories": {
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hearhellacopters/bireader.git"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --module esnext --outDir dist/esm && mv dist/esm/bireader.js dist/esm/bireader.mjs"
  },
  "keywords": [
    "buffer",
    "parser",
    "reader",
    "binary",
    "decode",
    "unpack",
    "struct",
    "bit",
    "hex"
  ],
  "author": "HearHellacopters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hearhellacopters/bireader/issues"
  },
  "homepage": "https://github.com/hearhellacopters/bireader#readme"
}
